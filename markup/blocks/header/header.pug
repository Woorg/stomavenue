mixin header(data)
  header.header
    .header__in.in(class="uk-container uk-flex uk-flex-center@s uk-flex-left@m uk-flex-wrap")
      a.header__logo.logo(href="#")
        img(src='assets/images/general/logo.png' srcset='assets/images/general/logo@2x.png 2x, assets/images/general/logo@3x.png 3x' alt="Стомавеню")
      .header__contacts
        a.header__contacts-phone(href="#") +7 (499) 50 100 50
        .header__contacts-address г. Москва, Сытинский переулок, дом 10/5, строение 3
  // end header